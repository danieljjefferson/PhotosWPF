<Window x:Class="PhotosWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Name="Source" HorizontalAlignment="Left" Height="23" Margin="85,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="336"/>
        <TextBox Name="Destination" HorizontalAlignment="Left" Height="23" Margin="85,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="336"/>
        <TextBox Name="LogBox" Margin="10,138,10,10" TextWrapping="Wrap"/>
        <Button Name="GoBtn" Content="Go" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Width="75" Click="Go_Click" Height="23" IsEnabled="False"/>
        <Label Content="Source" HorizontalAlignment="Left" Margin="34,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Destination" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
        <Button Content="Dump" HorizontalAlignment="Left" Margin="426,10,0,0" VerticalAlignment="Top" Width="81" Height="23" Click="DumpBtn_Click"/>
        <Button Content="Photos" HorizontalAlignment="Left" Margin="426,38,0,0" VerticalAlignment="Top" Width="81" Height="23" Click="PhotosBtn_Click"/>
        <CheckBox Name="IsVideos" Content="Videos" HorizontalAlignment="Left" Margin="451,66,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
